{
  "version": 2,
  "builds": [
    {
      "src": "frontend/expense-tracker/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "backend/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/api/(.*)",
      "dest": "/backend/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/expense-tracker/dist/$1"
    }
  ]
}
