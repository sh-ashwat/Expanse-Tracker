{
    "version": 2,
    "builds": [
      { "src": "frontend/expense-tracker/package.json", "use": "@vercel/static-build" },
      { "src": "backend/**", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/backend/$1" },
      { "src": "/(.*)", "dest": "frontend/expense-tracker/$1" }
    ]
  }
  